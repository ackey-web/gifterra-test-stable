{
  "buildCommand": "pnpm install --shamefully-hoist && pnpm build",
  "installCommand": "pnpm install --shamefully-hoist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}